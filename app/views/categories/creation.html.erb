  <%= form_with model: @category, class: "category-form", url: '/categories/create' do |f| %>
  <div class="white box">
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
  </div>
    <%= f.submit "Save", class: "button has-text-white" %>
    </div>
      </div>
    <%= render "devise/shared/error_messages", resource: @category %>
  <% end %>
</div>
